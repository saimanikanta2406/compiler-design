%{
#include <stdio.h>
%}

%%
\<[^>]+\>     { printf("%s\n", yytext); }
.|\n          { /* Ignore other content */ }
%%

int main()
{
    FILE *fp = fopen("sample.html", "r");

    if(fp == NULL)
    {
        printf("Error: Cannot open sample.html\n");
        return 1;
    }

    yyin = fp;   /* Set input to file */
    yylex();

    fclose(fp);
    return 0;
}

int yywrap()
{
    return 1;
}
